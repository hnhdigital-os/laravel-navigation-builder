{
    "name": "hnhdigital-os/laravel-navigation-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9.0",
        "hnhdigital-os/laravel-html-generator": "^3.0",
        "hnhdigital-os/laravel-number-converter": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "illuminate/support": "^9.0"
    },
    "support": {
        "issues": "https://github.com/hnhdigital-os/laravel-navigation-builder/issues"
    },
    "autoload": {
        "psr-4": {
            "HnhDigital\\NavigationBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HnhDigital\\NavigationBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HnhDigital\\NavigationBuilder\\ServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 2000,
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        }
    },
    "prefer-stable": true,
    "minimum-stability" : "dev"
}
